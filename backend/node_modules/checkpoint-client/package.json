    "test.watch": "mocha 'dist/**/*_test.js' -w --timeout 10s",
    "prepublish": "tsc && prettier src/**/* -c && rollup -c && mocha 'dist/**/*_test.js' --timeout 10s"
  },
  "license": "MIT",
