    "globby": "11.0.1",
    "is-ci": "2.0.0",
    "mocha": "7.2.0",
    "prettier": "2.0.5",
    "rollup": "2.13.1",
    "tempfile": "3.0.0",
    "testaway": "2.0.1",
    "ts-node": "8.10.2",
    "typescript": "3.9.5"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "trailingComma": "all",
